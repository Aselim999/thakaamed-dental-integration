        <script>var dbConn;
var result;
var patientsArray = [];

try {
    // Create a PostgreSQL database connection
    dbConn = DatabaseConnectionFactory.createDatabaseConnection(
        &quot;org.postgresql.Driver&quot;,
        &quot;jdbc:postgresql://localhost:5432/thakaamed&quot;,
        &quot;abdallaselim&quot;,
        &quot;admin&quot;
    );
    
    // Define your query
    var query =
        &quot;SELECT o.order_id, o.patient_id, o.procedure_code, o.procedure_name &quot; +
        &quot;FROM imaging_orders o &quot; +
        &quot;LEFT JOIN ai_analysis_results a ON o.order_id = a.order_id &quot; +
        &quot;WHERE o.order_status = &apos;COMPLETED&apos; &quot; +
        &quot;AND a.analysis_id IS NULL &quot; +
        &quot;LIMIT 10&quot;;
    
    logger.info(&quot;Executing PostgreSQL query: &quot; + query);
    
    // Execute the query without parameters
    result = dbConn.executeCachedQuery(query);
    
    var metaData = result.getMetaData();
    var columnCount = metaData.getColumnCount();
    
    // Process the result set
    while (result.next()) {
        var patientObject = {};
        for (var i = 1; i &lt;= columnCount; i++) {
            var columnName = metaData.getColumnName(i);
            patientObject[columnName.toLowerCase()] = result.getString(i);
        }
        patientsArray.push(patientObject);
    }
    
    logger.info(&quot;Successfully retrieved &quot; + patientsArray.length + &quot; records&quot;);
    
    // Set the channel response
    channelMap.put(&apos;responseContentType&apos;, &apos;application/json&apos;);
    channelMap.put(&apos;responseCode&apos;, &apos;200&apos;);
    return JSON.stringify(patientsArray);
    
} catch (e) {
    logger.error(&quot;An error occurred: &quot; + e);
    channelMap.put(&apos;responseContentType&apos;, &apos;application/json&apos;);
    channelMap.put(&apos;responseCode&apos;, &apos;500&apos;);
    return JSON.stringify({
        error: &quot;Database query failed&quot;,
        message: e.toString()
    });
    
} finally {
    // Safely close result set
    if (result) {
        try {
            if (!result.isClosed()) {
                result.close();
            }
        } catch (e) {
            logger.error(&quot;Failed to close result set: &quot; + e);
        }
    }
    
    // Safely close connection
    if (dbConn) {
        try {
            if (!dbConn.isClosed()) {
                dbConn.close();
            }
        } catch (e) {
            logger.error(&quot;Failed to close database connection: &quot; + e);
        }
    }
}</script>
