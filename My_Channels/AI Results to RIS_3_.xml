          <script>// Parse AI results
var aiResult = JSON.parse(msg.toString());
var orderId = aiResult.order_id;
var findings = aiResult.findings;
var confidence = aiResult.confidence_score;

// Generate ORU^R01 message
var oruMessage = &quot;MSH|^~\\&amp;|SAIF|THAKAAMED|RIS|RADIOLOGY|&quot; + DateUtil.getCurrentDate(&apos;yyyyMMddHHmmss&apos;) + 
                 &quot;||ORU^R01|AI&quot; + DateUtil.getCurrentDate(&apos;yyyyMMddHHmmss&apos;) + &quot;|P|2.5\r\n&quot;;

// Add PID segment (retrieve from database)
var patientQuery = &quot;SELECT * FROM patients p JOIN imaging_orders o ON p.patient_id = o.patient_id &quot; +
                   &quot;WHERE o.order_id = &apos;&quot; + orderId + &quot;&apos;&quot;;
var patientResult = $dbconn.executeCachedQuery(patientQuery);

if (patientResult.size() &gt; 0) {
    var patient = patientResult.get(0);
    oruMessage += &quot;PID|1||&quot; + patient.get(&apos;patient_id&apos;) + &quot;^^^HOSPITAL^MR||&quot; + 
                  patient.get(&apos;last_name&apos;) + &quot;^&quot; + patient.get(&apos;first_name&apos;) + &quot;||&quot; +
                  patient.get(&apos;date_of_birth&apos;) + &quot;|&quot; + patient.get(&apos;gender&apos;) + &quot;\r\n&quot;;
}

// Add OBR segment
oruMessage += &quot;OBR|1|&quot; + orderId + &quot;|&quot; + orderId + &quot;|PANO^Panoramic X-Ray^L|||&quot; +
              DateUtil.getCurrentDate(&apos;yyyyMMddHHmmss&apos;) + &quot;|||||||||||F\r\n&quot;;

// Add OBX segments for each finding
var findingIndex = 1;
for each (var finding in findings) {
    oruMessage += &quot;OBX|&quot; + findingIndex + &quot;|TX|AI_FINDING^AI Analysis^L||&quot; + 
                  finding.description + &quot; (confidence: &quot; + finding.confidence + &quot;%)||||||F\r\n&quot;;
    findingIndex++;
}

// Add overall confidence
oruMessage += &quot;OBX|&quot; + findingIndex + &quot;|NM|CONFIDENCE^Overall Confidence^L||&quot; + 
              confidence + &quot;|%|||||F\r\n&quot;;

channelMap.put(&apos;oruMessage&apos;, oruMessage);

// Update database
var sqlUpdate = &quot;UPDATE ai_analysis_results SET analysis_status = &apos;COMPLETED&apos;, &quot; +
                &quot;confidence_score = &quot; + confidence + &quot;, analysis_datetime = NOW() &quot; +
                &quot;WHERE order_id = &apos;&quot; + orderId + &quot;&apos;&quot;;
channelMap.put(&apos;sqlUpdate&apos;, sqlUpdate);</script>
        </com.mirth.connect.plugins.javascriptstep.JavaScriptStep>
      </elements>
