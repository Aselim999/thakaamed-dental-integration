          <script>// Parse DICOM worklist query parameters
var queryParams = msg.toString();
var response = [];

// Query scheduled procedures from database
var sql = &quot;SELECT o.*, p.* FROM imaging_orders o &quot; +
          &quot;JOIN patients p ON o.patient_id = p.patient_id &quot; +
          &quot;WHERE o.order_status = &apos;SCHEDULED&apos; &quot; +
          &quot;AND DATE(o.scheduled_datetime) = CURRENT_DATE&quot;;

var results = $dbconn.executeCachedQuery(sql);

for (var i = 0; i &lt; results.size(); i++) {
    var row = results.get(i);
    
    var worklistItem = {
        &quot;AccessionNumber&quot;: row.get(&apos;order_id&apos;),
        &quot;PatientID&quot;: row.get(&apos;patient_id&apos;),
        &quot;PatientName&quot;: row.get(&apos;last_name&apos;) + &quot;^&quot; + row.get(&apos;first_name&apos;),
        &quot;PatientBirthDate&quot;: row.get(&apos;date_of_birth&apos;),
        &quot;PatientSex&quot;: row.get(&apos;gender&apos;),
        &quot;StudyInstanceUID&quot;: generateUID(),
        &quot;RequestedProcedureID&quot;: row.get(&apos;order_id&apos;),
        &quot;RequestedProcedureDescription&quot;: row.get(&apos;procedure_name&apos;),
        &quot;ScheduledProcedureStepStartDate&quot;: formatDate(row.get(&apos;scheduled_datetime&apos;)),
        &quot;ScheduledProcedureStepStartTime&quot;: formatTime(row.get(&apos;scheduled_datetime&apos;)),
        &quot;Modality&quot;: mapProcedureToModality(row.get(&apos;procedure_code&apos;)),
        &quot;ScheduledPerformingPhysicianName&quot;: row.get(&apos;ordering_physician&apos;),
        &quot;ScheduledStationAETitle&quot;: &quot;DENTAL_WS&quot;
    };
    
    response.push(worklistItem);
}

// Helper functions
function generateUID() {
    return &quot;1.2.840.113619.2.55.3.&quot; + DateUtil.getCurrentDate(&apos;yyyyMMddHHmmssSSS&apos;);
}

function formatDate(datetime) {
    return DateUtil.formatDate(&apos;yyyyMMdd&apos;, datetime);
}

function formatTime(datetime) {
    return DateUtil.formatDate(&apos;HHmmss&apos;, datetime);
}

function mapProcedureToModality(code) {
    var modalityMap = {
        &apos;PANO&apos;: &apos;DX&apos;,  // Digital Radiography
        &apos;CBCT&apos;: &apos;CT&apos;,  // CT
        &apos;IO&apos;: &apos;DX&apos;,    // Intraoral
        &apos;CEPH&apos;: &apos;DX&apos;   // Cephalometric
    };
    return modalityMap[code] || &apos;OT&apos;;
}

channelMap.put(&apos;worklistResponse&apos;, JSON.stringify(response));</script>
        </com.mirth.connect.plugins.javascriptstep.JavaScriptStep>
